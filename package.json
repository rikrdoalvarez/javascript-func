{
    "name": "javascript-func",
    "version": "1.0.0",
    "description": "Javascript functions",
    "main": "index.js",
    "scripts": {
        "dev": "nodemon --trace-warnings ./index.js",
        "test": "cross-env NODE_ENV=debug jest --no-cache ./test/*.test.js",
        "test:coverage": "cross-env NODE_ENV=debug jest --coverage --no-cache ./test/*.test.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/rikrdoalvarez/javascript-func.git"
    },
    "keywords": [
        "Javascript"
    ],
    "author": "Ricardo Alvarez Nilo",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/rikrdoalvarez/javascript-func/issues"
    },
    "homepage": "https://github.com/rikrdoalvarez/javascript-func#readme",
    "devDependencies": {
        "jest": "^27.5.1",
        "nodemon": "^2.0.15"
    },
    "dependencies": {
        "bunyan": "^1.8.15",
        "bunyan-format": "^0.2.1",
        "chai": "^4.3.6",
        "cross-env": "^7.0.3",
        "crypto-js": "^4.1.1",
        "fs-extra": "^10.1.0",
        "jest-sonar-reporter": "^2.0.0",
        "mocha": "^9.2.2"
    },
    "jest": {
        "verbose": true,
        "coverageDirectory": "<rootDir>/coverage",
        "testResultsProcessor": "jest-sonar-reporter",
        "collectCoverage": true
    },
    "jestSonar": {
        "sonar56x": true,
        "reportPath": "coverage",
        "reportFile": "test-report.xml",
        "indent": 4
    }
}
